<Window x:Class="D_IDE.Dialogs.GotoDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Go to dialog" Height="116" Width="259" ResizeMode="NoResize" ShowInTaskbar="False">
    <Grid>
		<TextBox Height="23" Margin="12,12,12,0" Name="textBox1" VerticalAlignment="Top" PreviewTextInput="textBox1_PreviewTextInput" />
		<Button IsDefault="True" Content="Go" Margin="0,0,12,12" Name="button1" Height="23" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="68" Click="button1_Click" />
	</Grid>
</Window>
