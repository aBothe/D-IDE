<Window x:Class="D_IDE.Dialogs.GotoDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Go to line" Height="97" Width="268" ResizeMode="NoResize" ShowInTaskbar="False" WindowStyle="SingleBorderWindow">
    <Grid>
		<TextBox Margin="12,24,86,0" Name="textBox1" PreviewTextInput="textBox1_PreviewTextInput" Height="23" VerticalAlignment="Top" />
		<Button IsDefault="True" Content="Go" Margin="0,24,12,0" Name="button1" Height="23" VerticalAlignment="Top" HorizontalAlignment="Right" Width="68" Click="button1_Click" />
		<Label Content="Enter a line number:" Height="14" HorizontalAlignment="Left" Margin="13,6,0,0" Name="label1" VerticalAlignment="Top" Padding="0" Width="114" />
	</Grid>
</Window>
