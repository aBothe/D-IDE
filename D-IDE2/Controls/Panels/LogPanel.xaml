<DockableContent x:Class="D_IDE.Controls.Panels.LogPanel"
        xmlns="clr-namespace:AvalonDock;assembly=AvalonDock"
		xmlns:w="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Log" Height="325" Width="664" HideOnClose="True">
	<w:Grid>
		<w:TextBox Name="MainText" IsReadOnly="True" VerticalScrollBarVisibility="Visible" IsReadOnlyCaretVisible="True" Margin="0,21,0,0" />
		<w:Menu Height="23" Name="menu1" VerticalAlignment="Top">
			<w:ComboBox Width="90" Padding="4,0,4,4" SelectedIndex="1" HorizontalContentAlignment="Center">
				<w:ComboBoxItem Content="System" />
				<w:ComboBoxItem Content="Building" />
				<w:ComboBoxItem Content="Debugging" />
				<w:ComboBoxItem Content="Program output" />
			</w:ComboBox>
		</w:Menu>
	</w:Grid>
</DockableContent>
